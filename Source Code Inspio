function C_Image_Delete(id) {

console.log(obrazky);
if (obrazky[id]){
canvas.remove(obrazky[id]);
delete obrazky[id];
canvas.renderAll();
}
}

function C_Image_ClipCircle(id) {

if (id == 1) id = Object.keys(obrazky)[0];

if (obrazky[id]) {

obrazky[id].set({clipPath: null });

var radius = obrazky[id].width / 2;

obrazky[id].set({
clipPath: new fabric.Circle({
originX: 'center',
originY: 'center',
radius: radius
})
});

canvas.renderAll();
}
}

function C_Image2(X,Y,W,url,id) {
var img = new fabric.Image();

img.setSrc(url, function() {
img.top = Y;
img.left = X;
var radius = img.width / 2;

img.set({
clipPath: new fabric.Circle({
originX: 'center',
originY: 'center',
radius: radius
})
});
if (W) img.scaleToWidth(W);

img.lockMovementX = true;
img.lockMovementY = true;
img.lockScalingX = true;
img.lockScalingY = true;
img.lockRotation = true;

canvas.add(img);

obrazky[id] = img;

canvas.renderAll();
});
}
// vlozenie obrazku na stanovanu poziciu, obrazok sa cita z url a ukladat do objektu obrazky + id
function C_Image(X,Y,W,url,id,onload) {
var img = new fabric.Image();

img.setSrc(url, function() {
img.top = Y;
img.left = X;

if (W) img.scaleToWidth(W);

img.lockMovementX = true;
img.lockMovementY = true;
img.lockScalingX = true;
img.lockScalingY = true;
img.lockRotation = true;

canvas.add(img);

obrazky[id] = img;

Order_Images();

if (onload) onload();

canvas.renderAll();
});
}



// aktualizacia obrazku
function C_Image_Update(id,url,onload) {
obrazky[id].setSrc(url, function() {
if (onload) onload();
canvas.renderAll();
});
}

// zmena farebnosti obrazku, pouzity TINT, farba je v hexa zapise bez mriezky
function C_Image_ReColor(id,farba) {
var filter = new fabric.Image.filters.BlendColor({ color: farba, mode: 'tint' });
obrazky[id].filters.length = 0;
obrazky[id].filters.push(filter);
obrazky[id].applyFilters();
canvas.renderAll();
}

