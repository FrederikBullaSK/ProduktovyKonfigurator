$('#poznamka').addClass('d-hide');
if (konkretny_obrazok == custom_image) {
$('#poznamka').removeClass('d-hide');
$('#poznamka')[0].scrollIntoView(false);
if (custom_image_src!=""){
C_Image2(56,11,207,custom_image_src,'obrazok');
C_Image_ClipCircle('obrazok');
}
else  {
C_Image(1*usedOffset,40,600*usedKoef,'/content/' + sablona + '/obrazokd-' + konkretny_obrazok + '.png','obrazok');
}

}
else {
$("#add2cart-button").popover('hide');
C_Image(1*usedOffset,40,600*usedKoef,'/content/' + sablona + '/obrazokd-' + konkretny_obrazok + '.png','obrazok');
}


Vystup();
});
});
