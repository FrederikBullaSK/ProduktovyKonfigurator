$('#export-button').on('click', function() {
var export_filename = Math.random().toString(36).substr(2, 7);
obrazky.background.set({opacity: 0});
obrazky.vodoznak.set({opacity: 0});

var rect = new fabric.Rect({
left: 128,
top: 40,
fill: 'transparent',
width: 342,
height: 518,


type: 'cropper',
lockScalingX: true,
lockScalingY: true,
lockRotation: true
});
canvas.add(rect);

var i;
var croppedLeft = 0;
var croppedTop = 0;
var canvasJson = canvas.getObjects();
// Cropping canvas according to cropper rectangle
if (canvas.getObjects().length > 0) {
var i;
for (i = 0; i < canvas.getObjects().length; i++) {
if (canvas.getObjects()[i].type == 'cropper') {
croppedLeft = canvas.getObjects()[i].left;
croppedTop = canvas.getObjects()[i].top;
canvas.setHeight(canvas.getObjects()[i].height);
canvas.setWidth(canvas.getObjects()[i].width);
//    canvas.getObjects()[i].remove();
}
}
}
for (i = 0; i < canvasJson.length; i++) {
canvas.getObjects()[i].left = canvas.getObjects()[i].left - croppedLeft
canvas.getObjects()[i].top = canvas.getObjects()[i].top - croppedTop
canvas.renderAll();
}


if ($('#obrazok_toggles li.selected').data('obrazok') == custom_image  && data.crop == 1) obrazky.obrazok.set({opacity: 1});
else if ($('#obrazok_toggles li.selected').data('obrazok') == custom_image) obrazky.obrazok.set({opacity: 0});

obrazky.stojan1.set({opacity: 0});
obrazky.stojan2.set({opacity: 0});

$.ajax({

type: "POST",
url: "/save.php",
data: { type:"png",imgBase64: canvas.toDataURL({ format: 'png', quality: 1, multiplier: 5 }), filename: export_filename }
}).done(function(data) {
obrazky.vodoznak.set({opacity: 0.075});
if (data.noimage == 1) obrazky.obrazok.set({opacity: 1});


window.open(data);

canvas.renderAll();
}).fail(function() {
alert('');
});

return false;
});

$("input[name=material]").trigger("change");
