<script>

var rlimit = 12;
var timeout = null;

$('#s_key').on('input', function(e) {

var kw = $(this).val();

if (dataLayer) {  //GTM pridavanie
if (kw.length > 3) {
dataLayer.push({
'event': 'search',
'search_term': kw,
'search_string': kw
});
}
}

$('#inspio-search-result').html("");

clearTimeout(timeout);

timeout = setTimeout( function() {

var pattern = '<div class="col-xs-6 col-sm-4 col-md-3 col-lg-2"><a href="URL" title="TITLE"><h4>TITLE</h4><div class="inspio-image-holder" style="background-image: url(IMAGE)"></div><span>BASE</span></a></div>';

$.getJSON('/s/search.php',{
words: kw,
//    sort: so,
format: "json"
})
.done(function( data ) {


$('#inspio-search-result').html("");

if (data.status > 0) {

$.each( data.products, function( i, item ) {

var row = pattern;
row = row.replace('URL',item.url);
row = row.split('TITLE').join(item.title); //replace('',item.title);
row = row.replace('IMAGE',item.image);
row = row.replace('BASE',item.base);

if (i < rlimit) $('#inspio-search-result').append(row);
else return false;

});

if (data.status > rlimit) {

var vysledkov = data.status - rlimit;

$('#inspio-search-result').append('<div class="col-xs-12" align="center"><a href="/obchod/?referrer=search&keywords=' + kw + '" class="btn btn-success">Viac výsledkov (ešte ' + vysledkov + ' navyše)</a></div>');
}

}
else {

if (kw.length > 3)
$('#inspio-search-result').append('<div class="col-xs-12" align="center"><a href="/clanok/kontakt" class="notfound">Nepodarilo sa nám nájsť, čo hľadáte. <br>Nakreslíme niečo na mieru?</a></div>');

}

});

},200);	//timeout

});

</script>





</body>

</html>
