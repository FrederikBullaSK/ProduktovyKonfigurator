// prepinanie stojanov
var stojan_price = [0,4.9,7.9,8.9,12.9];

$('#stojan_toggles li').each(function() {
var konkretny_stojan = $(this).data('stojan');




$(this).on('click', function() {


var x = aktualne_cena;


if (aktualne.stojan!=konkretny_stojan) {
C_Image_Update('stojan1','/content/' + sablona + '/stand1-' + konkretny_stojan + '.png');
C_Image_Update('stojan2','/content/' + sablona + '/stand2-' + konkretny_stojan + '.png');

x = x*1+$(this).data('price')*1-stojan_price[aktualne.stojan]*1;

aktualne.stojan = konkretny_stojan;

aktualne_cena = floatval(x);
$("#stojan-desc").text($(this).data("desc1"));
}
$('#price').text(number_format(aktualne_cena));


$(this).siblings().removeClass('selected');
$(this).addClass('selected');
$(this).siblings().attr('data-selected','0');
$(this).attr('data-selected','1');

Vystup();
});
});


// prepinanie farebnosti textu a motivu
$('#farba_toggles li').each(function() {
var farba_prepinaca = $(this).data('farba');
$(this).children('span').css({backgroundColor: '#' + farba_prepinaca});

$(this).on('click', function() {
aktualne.farba = '#' + farba_prepinaca;
console.log("farba",aktualne.farba,obj.glass);
for (i of Object.keys(obj.glass)) {
if (udaje[obj.glass[i].text]) {

udaje[obj.glass[i].text].set({fill: aktualne.farba,color: aktualne.farba});

}

}


//  C_Image_ReColor('motiv', aktualne.farba);
$(this).siblings().removeClass('selected');
$(this).addClass('selected');
//alert(aktualne.farba);
Vystup();
});
});

// prepinanie motivu
$('#motiv_toggles li').each(function() {
