<script>

function GTM_RemoveFromCart(id,price,quantity = 1) {
if (dataLayer) {  //GTM odobratie z kosika
dataLayer.push({ ecommerce: null });
dataLayer.push({
'event': 'remove_from_cart',
'ecommerce': {
'currency': 'EUR',
'value': price,
'items': [
{
'item_id': id,
'id': id,
'price': price,
'item_brand': 'INSPIO',
'quantity': quantity
}],
'currencyCode': 'EUR',
'remove': {                                // 'remove' actionFieldObject measures.
'products': [{                        //  removing a product to a shopping cart.

'id': id,
'price': price,
'quantity': quantity
}]
}
}
});
}
}

$(document).ready(function() {
var jerstart = 2006;
var jercur = new Date();
jercur = jercur.getFullYear();
$('#jer').html(jercur);
$('#jerminus').html((jercur-jerstart));

$('a[href~="' + location.pathname + '"]').parents('li').addClass('current');
$('#inspio-search-form-close').on('click',function(e) {
e.preventDefault();
$('.inspio-search-container').toggleClass('hidden');
});
$('#inspio-search-form-submit').on('click',function(e) {
e.preventDefault();
$('#inspio-search-form').submit();
});

//CMP text
if ($('#cc-nb-title')) {
$('.termsfeed-com---nb .cc-nb-main-container').css('padding','2.2em');
$('#cc-nb-title').html('Aj my používame <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="0 0 24 24" stroke-width="1.5" stroke="#EB5532" fill="none" stroke-linecap="round" stroke-linejoin="round" xmlns:v="https://vecta.io/nano" style="vertical-align: top;"><path d="M8 13v.01M12 17v.01M12 12v.01M16 14v.01M11 8v.01"/><path d="M13.148 3.476l2.667 1.104a4 4 0 0 0 4.656 6.14l.053.132a3 3 0 0 1 0 2.296c-.497.786-.838 1.404-1.024 1.852-.189.456-.409 1.194-.66 2.216a3 3 0 0 1-1.624 1.623L15 19.5c-.475.197-1.092.538-1.852 1.024a3 3 0 0 1-2.296 0C10.05 20.021 9.433 19.68 9 19.5c-.471-.195-1.21-.415-2.216-.66a3 3 0 0 1-1.623-1.624L4.5 15c-.198-.479-.54-1.096-1.024-1.852a3 3 0 0 1 0-2.296c.48-.744.82-1.361 1.024-1.852.171-.413.391-1.152.66-2.216a3 3 0 0 1 1.624-1.623L9 4.5c.458-.19 1.075-.531 1.852-1.024a3 3 0 0 1 2.296 0z"/></svg>');
$('#cc-nb-text').html('Aj my ich používame. Ako všetky e-shopy na svete. Neukladáme do nich žiadne vaše osobné údaje, ale ak chcete, môžete niektoré z nich vypnúť.');
