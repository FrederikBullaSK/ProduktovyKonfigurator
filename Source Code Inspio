if ($('#' + k + '_toggles').length) {
$('#' + k + '_toggles li').removeClass('selected');

$('#' + k + '_toggles li').attr('data-selected',0);
$('#' + k + '_toggles li[data-' + k + '="' + v + '"]').addClass('selected');
$('#' + k + '_toggles li[data-' + k + '="' + v + '"]').attr('data-selected',1);
} else if ($('input[name="' + k + '"][type=checkbox]').length) {
$('input[name="' + k + '"][type=checkbox]').prop('checked', v > 0);
} else {
$('input[name="' + k + '"], select[name="' + k + '"], textarea[name="' + k + '"]').val(v);
}
});
}





$(window).on('resize orientationchange', function() {
var sirka_okna = $("#canvas-holder").width() - 2;

if (sirka_okna < sirka_canvasu) {
canvas.setWidth(sirka_okna).setHeight(sirka_okna / pomer_canvasu);
canvas.setZoom(sirka_okna / sirka_canvasu);
}
});

$(window).trigger('resize');

//zakazane prave tlacidl
$('body').on('contextmenu', function(e) {
if (e.target.nodeName != "INPUT" && e.target.nodeName != "TEXTAREA")
e.preventDefault();
});

function Vystup() {
if ($('#obrazok_toggles li.selected').data('obrazok') == custom_image && price_2)
$('#price').text(number_format(price_2, zaokruhlenie, ',', ' '));
else
$('#price').text(number_format(price_1, zaokruhlenie, ',', ' '));

var vystupnedata = {};

$('#policka .form-input').each(function() {
var k = $(this).attr('name');
var v = $(this).val();
vystupnedata[k] = v + '';
});

aktualne.farba = aktualne.farba.replace('#', '');

$.each(aktualne, function(k, v) {
vystupnedata[k] = v + '';
});

vystupnedata['spagat'] = $('input[name="spagat"]').prop('checked') ? '1' : '0';
vystupnedata['oramovanie'] = $('input[name="oramovanie"]').prop('checked') ? '1' : '0';

vystupnedata['sablona'] = sablona;
let colors = ['BEZ','SK','DR','CI','CI'];
let scode = "";
ccode = colors[aktualne.stojan];
const maxS = 3;
if (aktualne.stojan==0) scode = '0';
else if (aktualne.stojan>maxS) scode = 'R'+(aktualne.stojan-maxS);
else scode = 'S'+aktualne.stojan;
vystupnedata['code'] = 'S23'+scode+ccode;
vystupnedata['material'] = 'glass';
if ($('#poznamka').is(':visible') && $('#vlastnafoto').val()) {
vystupnedata['photo_load'] = 'VLASTNAFOTO_FILE';
vystupnedata['photo_orig'] = 'VLASTNAFOTOORIG_FILE';
}
//console.log(vystupnedata);
$('#price').text(number_format(aktualne_cena, zaokruhlenie, ',', ' '));
//  $('#vystup span').html(translate_from_sk(JSON.stringify(vystupnedata).split('","').join('", "')));
$('#vystup span').html(JSON.stringify(vystupnedata).split('","').join('", "'));

}

// vlozenie textu na stanovenu poziciu, pocita sa od laveho horneho rohu, S je velkost fontu v pixeloch, A je uhol otocenia textu
function Info_o_Boxe(textbox) {
var uhol = "";
if (Math.abs(textbox.angle) > 0) uhol = ', uhol: ' + Math.round(textbox.angle*100)/100 + ' &deg;';
return '<strong>' + textbox.text + '</strong> - Ľavo:' + Math.round(textbox.oCoords.tl.x) + ', Hore:' + Math.round(textbox.oCoords.tl.y) + ', Šir.:' + Math.round(textbox.width) + ', ' + textbox.fontFamily + ' / ' + Math.round(textbox.fontSize*100)/100 + 'pt / ' + textbox.textAlign + uhol;
}

function C_Text2(X,Y,S,A,font,string,boxwidth,align) {
var textbox = new fabric.Textbox(string, {
top: Y,
left: X,
fontSize: S,
angle: A,
textAlign: align,
width: boxwidth,
maxWidth: boxwidth,
editable: editacia,
fontFamily: font
});
canvas.add(textbox);
canvas.renderAll();

textbox.on('modified',function(e) {
$('#log').html( Info_o_Boxe(textbox) );
});
textbox.on('selected',function(e) {
$('#log').html( Info_o_Boxe(textbox) );
});
textbox.on('changed',function(e) {
$('#log').html( Info_o_Boxe(textbox) );
});

return textbox;
}





function ownPhoto(transl,X,Y,W){


Dropzone.options.myDropzone = {
url: '../upload.php',
maxFiles: 1,
paramName: "cropfile",
addRemoveLinks: true,
dictDefaultMessage: transl.dict_DefaultMessage,
dictFallbackMessage: transl.dict_FallbackMessage,
dictFallbackText: transl.dict_FallbackText,
dictFileTooBig: transl.dict_FileTooBig,
dictInvalidFileType: transl.dict_InvalidFileType,
dictResponseError: transl.dict_ResponseError,
dictCancelUpload: transl.dict_CancelUpload,
dictCancelUploadConfirmation: transl.dict_CancelUploadConfirmation,
dictRemoveFile: transl.dict_RemoveFile,
dictMaxFilesExceeded: transl.dict_MaxFilesExceeded,
dictCropConfirm: transl.dict_CropConfirm,
dictDimensionReject: transl.dict_DimensionReject,
dictFilesizeReject: transl.dict_FilesizeReject,
maxImageWidth: 4000,
maxImageHeight: 4000,
maxImageSizeMB: 5,
maxFilesize: 5, //MB
transformFile: function (file, done) {

var myDropZone = this;

// Create the image editor overlay
var editor = document.createElement("div");
editor.style.position = "fixed";
editor.style.left = 0;
editor.style.right = 0;
editor.style.top = 0;
editor.style.bottom = 0;
editor.style.zIndex = 9999;
editor.style.backgroundColor = "#000";

// Create the confirm button
var confirm = document.createElement("button");
confirm.classList.add("btn");
confirm.classList.add("btn-primary");
confirm.classList.add("btn-sm");
confirm.classList.add("mt--10");
confirm.classList.add("mb--10");
confirm.style.position = "absolute";

confirm.style.marginLeft = "-50px";
confirm.style.bottom = "15px";
confirm.style.width = "200px";
confirm.style.left = "50%";
confirm.style.zIndex = 9999;
confirm.textContent = myDropZone.options.dictCropConfirm;
confirm.addEventListener("click", function () {

// Get the canvas with image data from Cropper.js
var canvas = cropper.getCroppedCanvas({
minWidth: 800,
minHeight: 800,
maxWidth: 1072,
maxHeight: 1072 });

// Turn the canvas into a Blob (file object without a name)
canvas.toBlob(function (blob) {

// Update the image thumbnail with the new image data
myDropZone.createThumbnail(
blob,
myDropZone.options.thumbnailWidth,
myDropZone.options.thumbnailHeight,
myDropZone.options.thumbnailMethod,
false,
function (dataURL) {

// Update the Dropzone file thumbnail
myDropZone.emit("thumbnail", file, dataURL);

//console.log(file);
//console.log(dataURL);

// Return modified file to dropzone
done(blob);
}
);


});

// Remove the editor from view
editor.parentNode.removeChild(editor);

});
editor.appendChild(confirm);

// Load the image
var image = new Image();
image.src = URL.createObjectURL(file);
editor.appendChild(image);

// Append the editor to the page
document.body.appendChild(editor);

fileOriginal = file;
// Create Cropper.js and pass image
var cropper = new Cropper(image, {
aspectRatio: 1 });

},
init: function() {
//this.on("addedfile", function(file) { console.log("Added file."); });
this.on("addedfile", function(file){  // Ak je limit 1 subor, novy upload premaze povodny
$("#add2cart-button").popover('hide');
if (this.options.maxFiles == 1) {
if (this.fileTracker) {
this.removeFile(this.fileTracker);
}
this.fileTracker = file;
}

fileOriginal = file;

//   console.log("add",fileOriginal.dataURL);
$("#file").val(fileOriginal.dataURL);
//  console.log("foto",$("#file").val());
});
this.on("maxfilesexceeded", function(file) { this.removeFile(file); });
this.on("thumbnail",function(file) {
//console.log("thumbnail");
//console.log(file);
//  console.log("thumbnail",file);
$("#file").val(file.dataURL);
//   console.log("foto",$("#file").val());
});
this.on("success",function(file,response) {
//console.log("success");
//console.log(file);

// console.log(response);
$("#original").val(response.original);
$("#vlastnafoto").val(response.cropped);
custom_image_src = response.path;

// C_Image_Update("obrazok",custom_image_src);

C_Image_Delete("obrazok");
//C_Image2(20,10,280,custom_image_src,"obrazok"); //size
C_Image2(X,Y,W,custom_image_src,"obrazok"); //size
C_Image_ClipCircle("obrazok");
Order_Images();
Vystup();
})
this.on("removedfile", function(file) {
//console.log("Removed file.");
C_Image_Delete("obrazok");
custom_image_src = "";
});
}

};


}

var price_1 = floatval('37,00&nbsp;€');
var price_2 = floatval('');


var font_1 = new FontFaceObserver('baloo');
var font_2 = new FontFaceObserver('throw');
var font_3 = new FontFaceObserver('stylishcaligraphy');
var font_4 = new FontFaceObserver('GreatVibes');
var font_5 = new FontFaceObserver('YellowSun');

//    setBoard();
var aktualne_cena = price_1;

Promise.all([font_1.load(), font_2.load(), font_3.load(), font_4.load(), font_5.load()]).then(function() {

aktualne.farba = '#' + $('#farba_toggles li.selected').data('farba');
aktualne.motiv = $('#motiv_toggles li.selected').data('motiv');
aktualne.pozadie = $('#pozadie_toggles li.selected').data('pozadie');
aktualne.obrazok = $('#obrazok_toggles li.selected').data('obrazok');
