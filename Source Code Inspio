//hodnota = hodnota.replace(/([\u2700-\u27BF]|[\uE000-\uF8FF]|\uD83C[\uDC00-\uDFFF]|\uD83D[\uDC00-\uDFFF]|[\u202B-\u26FF]|\uD83E[\uDD10-\uDDFF])/g, ''); //emoji
