var novenaznak = udaje[polozka].width / (udaje[polozka].text.length + 5);
var novypomer = novenaznak / naznak;
udaje[polozka].fontSize *= 0.8;
}

if (udaje[polozka].width > udaje[polozka].maxWidth) {
udaje[polozka].fontSize *= udaje[polozka].maxWidth / (udaje[polozka].width + 1);
udaje[polozka].width = udaje[polozka].maxWidth;
}
