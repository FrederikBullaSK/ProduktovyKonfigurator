<script>

var translate = {
"oramovanie":    "",
"sablona":  "",
"poznamka":  "",
"farba":   "",
"farba_pozadia":   "",
"motiv":   "",
"pozadie": "",
"dierky":"",
"ram_plagatu":   "",
"obrazok": "",
"spagat":  ""};

function translate_from_sk(s) {
$.each(translate, function(k, v) {
s = s.replace('"' + k + '":', '"' + v + '":');
});

return s;
}

function translate_to_sk(s) {
$.each(translate, function(k, v) {
s = s.replace('"' + v + '":', '"' + k + '":');
});

return s;
}

